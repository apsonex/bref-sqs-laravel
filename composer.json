{
  "name": "apsonex/bref-sqs-laravel",
  "description": "Laravel adapter for bref",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Christoph Kluge",
      "email": "work@christoph-kluge.eu"
    }
  ],
  "require": {
    "bref/bref": "^1.4",
    "laravel/framework": "^8.76"
  },
  "autoload": {
    "psr-4": {
      "Apsonex\\BrefSqsLaravel\\Sqs\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apsonex\\BrefSqsLaravel\\Tests\\Sqs\\Laravel\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Apsonex\\BrefSqsLaravel\\Sqs\\Laravel\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}